choice
prompt "select sample rate:"
depends on USE_AMR
default USE_AMR_NB
    config USE_AMR_NB
        depends on USE_AMR
        bool "use 8k samplerate dec"
        help
            use amr decoder
    config USE_AMR_WB
        depends on USE_AMR
        bool "use 16k samplerate dec"
        help
            use amr decoder
endchoice

